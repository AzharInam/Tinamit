[run]
source = tinamit
omit =
       */trads/*
       */Interfaz/*
       */local/*
       */Ejemplos/*
       */Incertidumbre/*

[report]
omit =
       */trads/*
       */Interfaz/*
       */local/*
       */Ejemplos/*
       */Incertidumbre/*

exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.: